package com.imooc.set;

import java.util.HashSet;
import java.util.Set;

public class WordDemo {

	public static void main(String[] args) {
		//将英文单词添加到HashSet中
		Set set = new HashSet();
		//向集合添加元素
		set.add("blue");
		set.add("red");
		set.add("black");
		set.add("yellow");
		set.add("white");
		//显示集合的内容
		System.out.println("集合中的元素为：");
	}

}
