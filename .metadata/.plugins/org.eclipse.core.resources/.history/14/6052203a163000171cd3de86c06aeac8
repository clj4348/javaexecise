package com.imooc.work;

public class Work {
	//属性:工作类型
	private String type;
	
	//无参构造方法，完成工作类型的赋值
	public Work(){
		
	}
	//公有的get/set方法完成属性封装
	public String getType(){
		return type;
	}
	public void setType(String type){
		this.type = type;
	}
	//方法：工作描述，描述内容为：开心工作
	public String Work(){
		return "开心工作";
	}
}
